class Solution{
    public int[] twoSum(int[] arr,int tar){
        for(int i=0;i<arr.length;i++){
            for(int j=i+1;j<arr.length;i++){
                if((arr[i]+arr[j]==tar)){
                    return new int[]{i,j};
                }
            }
        }
        return new int[] {};
    }
}
